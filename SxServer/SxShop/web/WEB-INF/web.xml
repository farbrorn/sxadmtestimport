<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SxShopRPC</servlet-name>
        <servlet-class>se.saljex.SxShop.server.SxShopRPCImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getdoc</servlet-name>
        <servlet-class>se.saljex.SxShop.server.getdoc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SxShopRPC</servlet-name>
        <url-pattern>/se.saljex.SxShop.Main/sxshoprpc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getdoc</servlet-name>
        <url-pattern>/getdoc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>welcomeGWT.html</welcome-file>
    </welcome-file-list>
</web-app>
